{
  "name": "angular-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:aot": "ng serve --aot",
    "lint": "ng lint",
    "test": "ng test --watch false --code-coverage",
    "test:watch": "ng test --code-coverage",
    "test:watch:chrome": "ng test --browsers Chrome --code-coverage",
    "e2e": "ng e2e",
    "config": "ts-node ./set-env.ts",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "build:aot": "ng build --aot --build-optimizer",
    "build:aot:watch": "ng build --aot --watch",
    "build:prod": "ng build --configuration production --prod",
    "pipeline": "npm install && npm run lint && npm test && npm run build:prod",
    "sonar": "node sonar-project.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@auth0/angular-jwt": "^3.0.1",
    "bootstrap": "^4.4.1",
    "br-mask": "0.0.10",
    "chart.js": "^2.9.3",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "js-base64": "^3.4.5",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "msal": "^1.3.3",
    "ng-recaptcha": "^5.0.0",
    "ng2-currency-mask": "^9.0.2",
    "primeflex": "^2.0.0",
    "primeicons": "^2.0.0",
    "primeng": "^8.1.1",
    "quill": "^1.3.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.5.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~13.1.8",
    "codelyzer": "^5.2.1",
    "dotenv": "^8.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "protractor": "~5.4.2",
    "protractor-angular-screenshot-reporter": "^0.1.2",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.5.0",
    "yargs": "^15.4.1"
  }
}
